      <material name="white"/>
    </visual>
    <inertial>
      <inertia ixx= "1.299e-5" ixy= "5.557e-10" ixz= "-2.973e-10" iyy= "2.323e-5" iyz= "-8.199e-8" izz= "2.227e-5" ></inertia>
      <mass value="0.036"></mass>
    </inertial>
