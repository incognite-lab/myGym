<?xml version="1.0"?>
<robot name="Reachy">

  <material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <material name="red">
    <color rgba="0.8 0 0 1"/>
  </material>

<link name="world" />
<link name="base">
    <visual>
      <origin xyz="0 0 0.3" rpy="0 0 0"></origin>
      <geometry>
        <mesh filename="package://reachy/meshes/trunk-visual.stl"></mesh>
      </geometry>
      <material name="blue"/>
    </visual>
    <inertial>
      <inertia ixx= "1.043e-4" ixy= "6.147e-6" ixz= "-1.971e-6" iyy= "1.364e-4" iyz= "1.466e-5" izz= "1.403e-4" ></inertia>
      <mass value="0.221"></mass>
    </inertial>
  </link>
  <joint name="world_to_base_link" type="fixed">
    <parent link="world"/>
    <child link="base"/>
  </joint>
     <link name="shoulder">
