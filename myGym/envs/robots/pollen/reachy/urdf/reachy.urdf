    </collision>
  </link>
    <link name="ee_x_axis">
    <visual>
    <origin xyz="0.03 0 0" rpy="0 1.57079632679 0"/>
    <geometry>
      <cylinder length="0.06" radius="0.002"/>
    </geometry>
    <material name="red">
      <color rgba="1 0 0 1"/>
    </material>
  </visual>
    </link>
    <link name="ee_y_axis">
  <visual>
    <origin xyz="0 0.03 0" rpy="-1.57079632679 0 0"/>
    <geometry>
      <cylinder length="0.06" radius="0.002"/>
    </geometry>
    <material name="green">
      <color rgba="0 1 0 1"/>
    </material>
  </visual>
    </link>
    <link name="ee_z_axis">
  <visual>
    <origin xyz="0 0 0.03" rpy="0 0 0"/>
    <geometry>
      <cylinder length="0.06" radius="0.002"/>
    </geometry>
    <material name="blue">
      <color rgba="0 0 1 1"/>
    </material>
  </visual>
</link>
    <link name="endeffector">
  <visual>
    <origin xyz="0 0 0.0" rpy="0 0 0"/>
    <geometry>
      <sphere radius="0.005"/>
    </geometry>
    <material name="yellow">
      <color rgba="1 1 0 1"/>
    </material>
  </visual>
</link>
<joint name="endeffector_joint" type="fixed">
    	<parent link="gripper"/>
    	<child link="endeffector"/>
    	<origin rpy="0 0 -1.57" xyz="0.0 0.0 -0.12"/> 
	</joint>
	<joint name="ee_z_axis_joint" type="fixed">
    	<parent link="gripper"/>
    	<child link="ee_z_axis"/>
    	<origin rpy="3.14 0 0" xyz="0.0 -0.0 -0.12"/>
	</joint>
    	<joint name="ee_x_axis_joint" type="fixed">
    	<parent link="gripper"/>
    	<child link="ee_x_axis"/>
    	<origin rpy="3.14 0 0" xyz="0.0 -0.0 -0.12"/>
	</joint>
    <joint name="ee_y_axis_joint" type="fixed">
    	<parent link="gripper"/>
    	<child link="ee_y_axis"/>
    	<origin rpy="3.14 0 0" xyz="0.0 -0.0 -0.12"/>
	</joint>



  <joint name="shoulder_pitch_rjoint" type="revolute">
    <parent link="base"/>
    <child link="shoulder"/>
    <origin rpy="0 -1.5 0" xyz="0 -0.14 0.3"/>
    <axis xyz="0 1 0"/>
    <limit effort="30" lower="-1.5708" upper="3.14159" velocity="1"/>
  </joint>

  <joint name="shoulder_roll_rjoint" type="revolute">
    <parent link="shoulder"/>
    <child link="shoulder_to_arm"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit effort="30" lower="-3.14159" upper="0.174533" velocity="1"/>
  </joint>

  <joint name="arm_yaw_rjoint" type="revolute">
    <parent link="shoulder_to_arm"/>
    <child link="upper_arm"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="30" lower="-1.5708" upper="1.5708" velocity="1"/>
  </joint>

  <joint name="elbow_pitch_rjoint" type="revolute">
    <parent link="upper_arm"/>
    <child link="forearm"/>
    <origin rpy="0 0 0" xyz="0 0 -0.30745"/>
    <axis xyz="0 1 0"/>
    <limit effort="30" lower="-2.18166" upper="0" velocity="1"/>
  </joint>

  <joint name="forearm_yaw_rjoint" type="revolute">
